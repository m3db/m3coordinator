{
  "Linters": {
    "badtime": {
      "Command": "badtime",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    },
    "importorder": {
      "Command": "importorder -patterns=\"STDLIB github.com/m3db/m3coordinator github.com/m3db EXTERNAL\" ./...",
      "Pattern": "PATH:LINE:MESSAGE"
    }
  },
  "Enable":
    [ "importorder" ],
  "Deadline": "3m",
  "EnableGC": true
}
